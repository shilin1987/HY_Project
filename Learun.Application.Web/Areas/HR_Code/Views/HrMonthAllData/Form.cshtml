@{
    ViewBag.Title = "菜价补贴月汇总数据";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">系统id<font face="宋体">*</font></div>
        <input id="oid" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">成本中心<font face="宋体">*</font></div>
        <input id="center_cost" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">成本中心人员合计</div>
        <input id="countperson" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">成本中心金额合计</div>
        <input id="summoney" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">平均金额</div>
        <input id="avgmoney" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_vpsmonthdataALL" >
        <div class="lr-form-item-title">备注</div>
        <input id="bk" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/HR_Code/Views/HrMonthAllData/Form.js")
