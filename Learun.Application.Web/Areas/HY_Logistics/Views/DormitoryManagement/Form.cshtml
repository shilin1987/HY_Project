@{
    ViewBag.Title = "宿舍管理功能";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_Candidates" >
        <div class="lr-form-item-title">姓名</div>
        <input id="F_RealName" type="text" class="form-control"   readonly="readonly"/>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_Candidates" >
        <div class="lr-form-item-title">身份证号</div>
        <input id="F_IDCard" type="text" class="form-control"  readonly="readonly" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="Hy_Recruit_Candidates">
        <div class="lr-form-item-title">性别</div>
        <div id="F_Gender" type="text" readonly="readonly"></div>
        @*<input id="F_Gender" type="text" class="form-control" readonly="readonly" />*@
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_Candidates" >
        <div class="lr-form-item-title">联系方式</div>
        <input id="F_Mobile" type="text" class="form-control"  readonly="readonly" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_Candidates" >
        <div class="lr-form-item-title">宿舍号</div>
        <input id="F_Dormitory" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/HY_Logistics/Views/DormitoryManagement/Form.js")
