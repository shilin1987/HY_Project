@{
    ViewBag.Title = "合同后台管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">合同名称<font face="宋体">*</font></div>
        <input id="F_Name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">合同类型<font face="宋体">*</font></div>
        <input id="F_Type" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">合同内容<font face="宋体">*</font></div>
        <div id="F_ContractContent" style="height:200px;"></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">内容预览</div>
        <input id="F_PreviewtheContract" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">附件上传</div>
        <div id="F_Url"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">创建人</div>
        <input id="F_CreateUserName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">创建时间</div>
        <input id="F_CreateDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_CreateDate').trigger('change'); } })"   />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">修改人</div>
        <input id="F_ModifyUserName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_Recruit_ContractTemplateManagement" >
        <div class="lr-form-item-title">修改时间</div>
        <input id="F_ModifyDate" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/Platform/Views/HyRecruitContractTemplateManagement/Form.js")
