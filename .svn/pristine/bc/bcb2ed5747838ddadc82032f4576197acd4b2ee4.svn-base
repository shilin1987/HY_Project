
.time-item strong {
    background: #C71C60;
    color: #fff;
    line-height: 29px;
    font-size: 16px;
    font-family: Arial;
    padding: 0 5px;
    margin-right: 5px;
    border-radius: 5px;
    box-shadow: 1px 1px 3px rgba(0,0,0,0.2);
}

#day_show {
    float: left;
    line-height: 20px;
    color: #c71c60;
    font-size: 20px;
    margin: 0 5px;
    font-family: Arial,Helvetica,sans-serif;
}

.item-title .unit {
    background: none;
    line-height: 20px;
    font-size: 16px;
    padding: 0 5px;
    float: left;
}
