<body class="hold-transition register-page">
    <div class="register-box">
        <div class="register-logo">
            <a href="all-admin-index.html"><b>华羿人员信息录入</b></a>
        </div>

        <div class="register-box-body">
            <p class="login-box-msg">人员信息录入</p>
            <form action="../../QrCodeInfo/Save" method="post">
                <div class="form-group has-feedback">
                    <input type="text" class="form-control" placeholder="姓名" name="userName">
                    <span class="glyphicon glyphicon-user form-control-feedback"></span>
                </div>
                <div class="col-md-6 title">性别</div>
                <div class="col-md-6 data">
                    <div class="form-group form-inline">
                        <div class="radio"><label><input type="radio" name="sex" value="1"> 男</label></div>
                        <div class="radio"><label><input type="radio" name="sex" value="2"> 女</label></div>
                    </div>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="form-control" placeholder="年龄" name="age">
                    <span class="glyphicon glyphicon-user form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="form-control" placeholder="身份证编号" name="cardId">
                    <span class="glyphicon glyphicon-user form-control-feedback"></span>
                </div>
                <div class="form-group has-feedback">
                    <input type="text" class="form-control" placeholder="联系方式" name="iphone">
                    <span class="glyphicon glyphicon-lock form-control-feedback"></span>
                </div>
                <div class="row">
                    <!-- /.col -->
                    <div class="col-xs-12">
                        <button type="submit" class="btn btn-primary btn-block btn-flat">保存</button>
                    </div>
                    <!-- /.col -->
                </div>
            </form>
        </div>
        <!-- /.form-box -->
    </div>
    <!-- /.register-box -->

    <!-- jQuery 2.2.3 -->
    <!-- Bootstrap 3.3.6 -->
    <!-- iCheck -->
    <script src="../plugins/jQuery/jquery-2.2.3.min.js"></script>
    <script src="../plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="../plugins/iCheck/icheck.min.js"></script>
    <script>
        $(function() {
            $('.btn btn-primary btn-block btn-flatinput').onclick({
                alert("1111111111111");
            });
        });
    </script>
</body>

</html>