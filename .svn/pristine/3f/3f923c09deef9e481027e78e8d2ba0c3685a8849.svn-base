@{
    ViewBag.Title = "考勤班次维护";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" style="display: none; " >
        <div class="lr-form-item-title">系统ID<font face="宋体">*</font></div>
        <input id="oid" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">员工编码<font face="宋体">*</font></div>
        <input id="userid" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">姓名<font face="宋体">*</font></div>
        <input id="username" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">班次</div>
        <input id="shift_no" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">考勤日期<font face="宋体">*</font></div>
        <input id="AttendanceDate" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">出勤时数</div>
        <input id="DateofAttendance" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">出差时数</div>
        <input id="AttendanceHours" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">外出时数</div>
        <input id="Outforhours" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">在岗时长</div>
        <input id="Onthejobworkhours" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_MonthlyDataSummary_file" >
        <div class="lr-form-item-title">上班制度<font face="宋体">*</font></div>
        <input id="WorkSystem" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/HR_Code/Views/MonthlyAttendanceDataClass/Form.js")
