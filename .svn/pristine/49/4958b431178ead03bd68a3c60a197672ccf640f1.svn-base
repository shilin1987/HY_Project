using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace WebApi_EmployeeInformationEntry.Models
{
    public class WrittenExaminationQuestionsModel
    {
        /// <summary>
        /// 题目
        /// </summary>
        public string Subject { get; set; }
        /// <summary>
        /// 选择答案
        /// </summary>
        public string Answer { get; set; }
        /// <summary>
        /// 正确答案
        /// </summary>
        public string RightKey { get; set; }

        /// <summary>
        /// 题目类别标识
        /// </summary>
        public int QuestionTypeFlag { get; set; }
        /// <summary>
        /// 题目类别
        /// </summary>
        public string QuestionType { get; set; }
        /// <summary>
        /// 这道题分数
        /// </summary>
        public int Score { get; set; }
        /// <summary>
        /// 所在题库
        /// </summary>
        public string QuestionsBank { get; set; }
        /// <summary>
        /// 这套题库及格分数线
        /// </summary>
        public int BankScore { get; set; }
    }
}