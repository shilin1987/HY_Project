<script type="text/javascript">
    $(function () {
        $("#frmContentDetainAdd").ajaxForm({
            beforeSubmit: function () {
                $.messager.progress({ title: '提示', msg: '系统处理中，请稍候……' });
                $("#btnDetainSubmit").val("提交中..");
                $("#btnDetainSubmit").attr("disabled", "disabled");
            }, success: function (response) {
                $.messager.alert("操作提示", response.Message, "info", function () {
                    $("#btnDetainSubmit").removeAttr("disabled");
                    $("#btnDetainSubmit").val("提交");
                    if (response.ResultType == 200) {
                        $("#openDetainWindow").dialog("close");
                        ApplyProductPage.doSeach();
                    } else {
                        $("#btnDetainSubmit").val("提交");
                        $("#btnDetainSubmit").removeAttr("disabled");
                    }
                });
                $.messager.progress('close');
            }
        });
    });
    function formDetainSubmit() {
        if ($("#DetainReason").val().trim() == '') {
            $.messager.alert("操作提示", "备注原因不能为空！");
            return;
        }
        $("#frmContentDetainAdd").submit();
    }

    //$(function () {
