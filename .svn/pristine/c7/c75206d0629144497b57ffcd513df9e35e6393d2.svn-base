@{
    ViewBag.Title = "水电费用结算";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="Hy_hr_WaterElectricity" >
        <div class="lr-form-item-title">员工账号<font face="宋体">*</font></div>
        <div id="F_UserId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_hr_WaterElectricity" >
        <div class="lr-form-item-title">结算日期<font face="宋体">*</font></div>
        <input id="F_SettlementYearMonth" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hy_hr_WaterElectricity" >
        <div class="lr-form-item-title">水电费用<font face="宋体">*</font></div>
        <input id="F_SettlementCost" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
</div>
@Html.AppendJsFile("/Areas/HR_Code/Views/WaterElectricity/Form.js")
