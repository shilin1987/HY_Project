@{
    ViewBag.Title = "身份证信息读取对比";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">姓名<font face="宋体">*</font></div>
        <input id="F_UserName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">身份证编号<font face="宋体">*</font></div>
        <input id="F_CardID" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">性别</div>
        <input id="F_Sex" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">当前状态<font face="宋体">*</font></div>
        <div id="F_State" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">最后操作日期<font face="宋体">*</font></div>
        <input id="F_OperatingTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_OperatingTime').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">操作人员<font face="宋体">*</font></div>
        <input id="F_OperationPerson" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="Hr_hy_ComparisonOfIdCardVerification" >
        <div class="lr-form-item-title">身份证读取差异</div>
        <textarea id="F_InformationContrastDifference" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/Platform/Views/ComparisonOfIdCardVerificationClass/Form.js")
