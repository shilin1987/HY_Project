@{
    ViewBag.Title = "员工手册后台管理";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">员工手册<font face="宋体">*</font></div>
        <input id="F_Name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">手册内容<font face="宋体">*</font></div>
        <input id="F_Content" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">附件上传</div>
        <div id="F_Url"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">内容预览<font face="宋体">*</font></div>
        <input id="F_Preview" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">版本<font face="宋体">*</font></div>
        <input id="F_Version" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">创建人<font face="宋体">*</font></div>
        <div id="F_CreateUserId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">创建时间<font face="宋体">*</font></div>
        <input id="F_CreateDate" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">修改人</div>
        <div id="F_ModifyUserId"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="Hy_Recruit_TheEmployeeHandbook" >
        <div class="lr-form-item-title">修改时间</div>
        <textarea id="F_ModifyDate" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/Platform/Views/Hy_Recruit_TheEmployeeHandbook/Form.js")
