/********接口警告信息样式***********/
.info {
    position: absolute;
    top: 0;
    right: 4px;
    font-size: 18px;
    line-height: 34px;
    color: #808080;
    cursor: help;
}

.info-message {
    position: absolute;
    bottom: 202px;
    right: 44px;
    background-color: #333A4C;
    padding: 6px;
    color: #fff;
    display: none;
    z-index: 99;
}

.table-info-message {
    bottom: 290px;
}

.info-message .ok {
    color: #1bb99a;
}

.info-message .error {
    color: #ff3015;
}

.step-gird-pane {
    padding:5px;
}
.lr-form-layout {
    padding-top: 190px;
}

    .lr-form-layout .lr-form-layout-header {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 190px;
        padding: 10px;
        border: 1px solid #ccc;
    }

    .lr-form-layout .lr-form-item-title {
        width: 58px;
        padding-right: 0px;
        text-align: left;
    }

    .lr-form-layout .lr-form-item {
        padding-left: 58px;
    }

    .lr-form-layout .lr-form-layout-body {
        padding-top: 30px;
        border: 1px solid #ccc;
        border-top: 0px;
    }