@{
    ViewBag.Title = "新员工入职信息临时保存";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">真实姓名<font face="宋体">*</font></div>
        <input id="F_RealName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">手机<font face="宋体">*</font></div>
        <input id="F_Mobile" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">备注<font face="宋体">*</font></div>
        <input id="F_Description" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">证件号码<font face="宋体">*</font></div>
        <input id="F_IDCard" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">招聘渠道<font face="宋体">*</font></div>
        <input id="F_RecruitmentChannels" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">学历<font face="宋体">*</font></div>
        <input id="F_Education" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">证件类型</div>
        <input id="F_DocumentType" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">国籍</div>
        <input id="F_NationAlity" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">民族</div>
        <input id="F_Nation" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">户籍性质</div>
        <input id="F_RRNature" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">籍贯</div>
        <input id="F_NativePlace" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">身份证开始日期</div>
        <input id="F_IDCardStartDate" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">身份证结束日期</div>
        <input id="F_IDCardEndDate" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">一卡通卡号</div>
        <input id="F_OneCardNumber" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">入职渠道<font face="宋体">*</font></div>
        <input id="F_EntryChannel" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">内部推荐人编码</div>
        <input id="F_InternalCode" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">内部推荐人姓名</div>
        <input id="F_InternalName" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">内部推荐人公司</div>
        <input id="F_InternalCompany" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">劳务转自招日期</div>
        <input id="F_LaborRecruitmentDate" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">代招公司</div>
        <input id="F_RecruitmentCompany" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item">
        <div class="lr-form-item-title">身份证地址<font face="宋体">*</font></div>
        <input id="F_IDCardAddress" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/Platform/Views/candidates/Form.js")
