<div style="margin-top:10px;" class="lr-form-container" id="userform">
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>年龄:</label>
		<input id="F_Age" type="text" isvalid="yes"  style="text-align: left;" checkexpession="NotNull" errormsg="年龄" onkeyup="(this.v=function(){this.value=this.value.replace(/[^0-9-]+/,'');}).call(this)" onblur="this.v();">
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>性别:</label>
		<div id="F_Gender" checkexpession="NotNull"  errormsg="性别" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>证件类型:</label>
		<div id="F_DocumentType" checkexpession="NotNull"  errormsg="证件类型" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>证件号码:</label>
		<input id="F_IDCard" type="text" style="text-align: left;" isvalid="yes" checkexpession="NotNull" errormsg="身份证号码">
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>国籍:</label>
		<div id="F_NationAlity" checkexpession="NotNull"  errormsg="国籍" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>民族:</label>
		<input id="F_Nation" type="text" style="text-align: left;" isvalid="yes" checkexpession="NotNull" errormsg="民族">
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>户籍性质:</label>
		<div id="F_RRNature" checkexpession="NotNull"  errormsg="户籍性质" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>手机号码:</label>
		<input id="F_Mobile" type="text" style="text-align: left;" isvalid="yes" checkexpession="NotNull" errormsg="手机号码">
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>政治面貌:</label>
		<div id="F_PoliticalOutlook" checkexpession="NotNull"  errormsg="政治面貌" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>婚姻状况:</label>
		<div id="F_MaritalStatus" checkexpession="NotNull"  errormsg="婚姻状况" ></div>
	</div>
	<div class="lr-form-row">
		<font face="宋体">*</font>
		<label>籍贯:</label>
		<div id="F_NativePlace" checkexpession="NotNull"  errormsg="籍贯" ></div>
	</div>
</div>

<div class="lr-list" style="margin-top:15px;">
	<div class="lr-list-item" style="color:red;text-align:center;" id="saveUser">
		保存
	</div>
</div>
