/********接口警告信息样式***********/
.info {
    position: absolute;
    top: 0;
    right: 4px;
    font-size: 18px;
    line-height: 34px;
    color: #808080;
    cursor: help;
}

.info-message {
    position: absolute;
    bottom: 202px;
    right: 44px;
    background-color: #333A4C;
    padding: 6px;
    color: #fff;
    display: none;
    z-index: 99;
}

.table-info-message {
    bottom: 290px;
}

.info-message .ok {
    color: #1bb99a;
}

.info-message .error {
    color: #ff3015;
}
.lr-form-item-text {
    position: absolute;
    top: 7px;
    right: 0;
    padding: 4px 8px;
    color: #000;
}