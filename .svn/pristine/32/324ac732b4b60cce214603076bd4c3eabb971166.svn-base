        <input id="F_MonBreakfastTogether" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="FS_VegetablePrices" >
        <div class="lr-form-item-title">月午餐补贴次数<font face="宋体">*</font></div>
        <input id="F_MonLunchTogether" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="FS_VegetablePrices" >
        <div class="lr-form-item-title">月晚餐补贴次数</div>
        <input id="F_MonDinnerTogether" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="FS_VegetablePrices" >
        <div class="lr-form-item-title">月夜宵补贴次数<font face="宋体">*</font></div>
        <input id="F_MonSupperTogether" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="FS_VegetablePrices" >
        <div class="lr-form-item-title">月补贴金额合计<font face="宋体">*</font></div>
        <input id="F_CountMoney" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
</div>
@Html.AppendJsFile("/Areas/HR_Code/Views/FSVegetablePricesCalculation/Form.js")
